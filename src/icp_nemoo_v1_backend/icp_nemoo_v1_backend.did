type FisherArg = record {
    id: text;
    name: text;
    city: text;
    age: int32;
};


type FishArg = record {
    id: text;
    fisher: text;
    weight: int32;
    height: int32;
};

service : {
    "get_command": () -> (text) query;
    "get_fisher": (text) -> (FisherArg) query;
    "get_fish": (text) -> (FishArg) query;
    "save_fisher": (FisherArg) -> (text);
    "save_fish": (FishArg) -> (text);
}
